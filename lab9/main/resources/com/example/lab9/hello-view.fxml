<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="169.0" prefWidth="218.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab9.HelloController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <AnchorPane prefHeight="165.0" prefWidth="246.0">
      <Button layoutX="73.0" layoutY="119.0" mnemonicParsing="false" onAction="#onChooseTaskMenu" text="Ок" />
      <RadioButton fx:id="radiobutton1" layoutX="15.0" layoutY="37.0" mnemonicParsing="false" text="Перше завдання">
         <toggleGroup>
            <ToggleGroup fx:id="TasksGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="radiobutton2" layoutX="18.0" layoutY="80.0" mnemonicParsing="false" text="Друге завдання" toggleGroup="$TasksGroup" />
   </AnchorPane>
</VBox>
